{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js", // You may need to adjust this path based on your project structure
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist" // Adjust this to match your Vite output directory
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/dist/$1" } // Adjust this route if necessary
    ]
  }
  